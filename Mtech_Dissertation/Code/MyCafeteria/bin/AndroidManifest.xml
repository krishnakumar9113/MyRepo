<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bits.cts.mycafeteria"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Index"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ItemLevelListActivity"
            android:label="@string/title_activity_item_level_list" 
             android:parentActivityName=".SecondLevelListActivity" >
        </activity>
        <activity
            android:name=".FavouritesListActivity">
        </activity>
        
        <activity
            android:name=".FirstLevelListActivity"
            android:label="@string/title_activity_first_level_list" >
        </activity>
        <activity
            android:name=".SecondLevelListActivity"
            android:label="@string/title_activity_second_level_list" >
        </activity>
        <activity
            android:name=".OrderListActivity"
            android:label="@string/title_activity_second_level_list" >
        </activity>
        <activity
            android:name=".OrderDescriptionActivity"
            android:label="@string/title_activity_order_description" >
        </activity>
        <activity
            android:name=".Cart"
            android:label="@string/title_activity_order_description" >
        </activity>
         <activity
            android:name=".UserDescriptionActivity"
            android:label="@string/title_activity_order_description" >
        </activity>
        
        
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
